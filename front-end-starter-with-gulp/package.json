{
  "name": "front-end-starter-with-gulp",
  "version": "1.2.0",
  "description": "This is a starter kit of the Web front-end development.",
  "author": "akabeko",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "web",
    "frontend",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akabekobeko/examples-web-app"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "esdoc": {
    "source": "./src/js",
    "destination": "./esdoc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "test": {
            "source": "./src/js"
          }
        }
      }
    ]
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register src/js/**/*.test.js",
    "start": "gulp watch",
    "esdoc": "esdoc",
    "eslint": "eslint ./src/js/",
    "server": "gulp server",
    "build:css": "gulp css",
    "build:js": "gulp js",
    "release": "gulp release"
  },
  "dependencies": {
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "del": "^3.0.0",
    "esdoc": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.7.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watchify": "^0.7.0",
    "mocha": "^3.5.0",
    "power-assert": "^1.4.4",
    "pretty-hrtime": "^1.0.3",
    "run-sequence": "^2.1.0",
    "uglify-es": "^3.0.28",
    "vinyl-buffer": "^1.0.0",
    "watchify": "^3.9.0"
  }
}
